{
  "name": "vizzy",
  "version": "1.0.0",
  "description": "A Reveal.js plugin to enhance iframe interactions and support PDF printing with transitions.",
  "main": "dist/vizzy.es5.js",
  "module": "dist/vizzy.es6.js",
  "scripts": {
    "build:es6": "babel src/vizzy.js -o dist/vizzy.es6.js --presets=@babel/preset-env",
    "build:es5": "babel src/vizzy.js -o dist/vizzy.es5.js --presets=@babel/preset-env --plugins=@babel/plugin-transform-modules-commonjs",
    "build": "npm run build:es6 && npm run build:es5",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Khlick/Vizzy.git"
  },
  "keywords": [
    "reveal.js",
    "plugin",
    "iframe",
    "transitions",
    "pdf"
  ],
  "author": "Khris Griffis, Ph.D.",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0"
  }
}
